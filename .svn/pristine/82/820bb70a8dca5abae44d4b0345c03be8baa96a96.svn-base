<div ng-cloak ng-if="isAuthenticated()">
	<section class="content-header">
		<h4 translate="main.title">Welcome, Wafa Cream !</h4>

	</section>
	<div class="row">
		<div class="row">
			<div class="col-lg-3 col-md-6">
				<div class="panel panel-primary">
					<div class="panel-heading with-border">
						<div class="row">
							<div class="col-xs-3">
								<i class="fa fa-comments fa-5x"></i>
							</div>
							<div class="col-xs-9 text-right">
								<div class="huge">26</div>
								<div>Mes Prospection !</div>
							</div>
						</div>
					</div>
					<a ui-sref="prospection">
						<div class="panel-footer">
							<span class="pull-left">View Details</span> <span
								class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
							<div class="clearfix"></div>
						</div>
					</a>
				</div>
			</div>
			<div class="col-lg-3 col-md-6">
				<div class="panel panel-warning">
					<div class="panel-heading with-border">
						<div class="row">
							<div class="col-xs-3">
								<i class="fa fa-tasks fa-5x"></i>
							</div>
							<div class="col-xs-9 text-right">
								<div class="huge">12</div>
								<div>Mes Compagne!</div>
							</div>
						</div>
					</div>
					<a ui-sref="compagne">
						<div class="panel-footer">
							<span class="pull-left">View Details</span> <span
								class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
							<div class="clearfix"></div>
						</div>
					</a>
				</div>
			</div>
			<div class="col-lg-3 col-md-6">
				<div class="panel panel-success">
					<div class="panel-heading with-border">
						<div class="row">
							<div class="col-xs-3">
								<i class="fa fa-user fa-5x"></i>
							</div>
							<div class="col-xs-9 text-right">
								<div class="huge">124</div>
								<div>Mes Prospects </div>
							</div>
						</div>
					</div>
					<a ui-sref="personne">
						<div class="panel-footer">
							<span class="pull-left">View Details</span> <span
								class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
							<div class="clearfix"></div>
						</div>
					</a>
				</div>
			</div>
			<div class="col-lg-3 col-md-6">
				<div class="panel panel-primary">
					<div class="panel-heading with-border">
						<div class="row">
							<div class="col-xs-3">
								<i class="fa fa-calendar fa-5x"></i>
							</div>
							<div class="col-xs-9 text-right">
								<div class="huge">13</div>
								<div>Mes Taches !</div>
							</div>
						</div>
					</div>
					<a ui-sref="tache">
						<div class="panel-footer">
							<span class="pull-left">View Details</span> <span
								class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
							<div class="clearfix"></div>
						</div>
					</a>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="  col-md-5">
				<div class="box box-success">
					<div class="box-header with-border">
						<h3 class="box-title">
							<i class="fa fa-clock-o fa-fw"></i> Corbeille
						</h3>
					</div>
					<div class="box-body">
						<div class="list-group">
							<a ui-sref="{{ tache.prospectionId!=null ? 'prospection.detail({id:tache.prospectionId,tacheId:tache.id})' : 'tache.detail({id:tache.id})' }}" class="list-group-item"
								ng-repeat="tache in LastTaches"> <span
								class="badge progress-bar-success">{{tache.dateFin}}</span> <span
								class="badge progress-bar-warning">{{tache.statutTacheLibelle}}</span>
								<i class="fa fa-fw fa-calendar"></i> {{tache.sujet}}

							</a>
						</div>
						<div class="text-right">
							<a ui-sref="tache">View All Activity <i
								class="fa fa-arrow-circle-right"></i></a>
						</div>
					</div>
				</div>
			</div>
			<div class="  col-md-7">
				<div class="box box-success">
					<div class="box-header with-border">
						<h3 class="box-title">
							<i class="fa fa-clock-o fa-fw"></i> Charts
						</h3>
					</div>
					<div class="box-body">
						<div class="row">
							<div class=" col-md-6">
								<canvas id="line" class="chart chart-line" chart-data="data1"
									chart-labels="labels1" chart-legend="true" chart-series="series1"
									chart-click="onClick">
							</canvas>
		
							</div>
		
							<div class=" col-md-6">
								<canvas id="bar" class="chart chart-bar" chart-data="data2"
									chart-labels="labels2"> chart-series="series"
							</canvas>
							</div>
							
							</div><div class="row">
							<div class="  col-md-6">
		
								<canvas id="doughnut" class="chart chart-doughnut"
									chart-data="data3" chart-labels="labels3">
							</canvas>
		
							</div>
		
							<div class=" col-md-6">
								<canvas id="radar" class="chart chart-radar" chart-data="data4"
									chart-labels="labels4">
						</canvas>
							</div>
		
						</div>
					</div>
				</div>
			</div>
		</div>
