<div>
    <h2 translate="creamApp.refSecteurActivite.home.title">RefSecteurActivites</h2>
    <jh-alert></jh-alert>
    <div class="container">
        <div class="row">
            <div class="col-md-4">
                <button class="btn btn-primary" ui-sref="refSecteurActivite.new">
                    <span class="glyphicon glyphicon-flash"></span> <span translate="creamApp.refSecteurActivite.home.createLabel">Create a new RefSecteurActivite</span>
                </button>
            </div>
            <div class="col-md-8">
                <form name="searchForm" class="form-inline">
                    <div class="form-group">
                        <input type="text" class="form-control" ng-model="searchQuery" id="searchQuery" placeholder="query">
                    </div>
                    <button class="btn btn-info" ng-click="search()"><span class="glyphicon glyphicon-search"></span> <span>Search a RefSecteurActivite</span>
                    </button>
                </form>
            </div>
        </div>
    </div>

    <div class="table-responsive">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th><span translate="global.field.id">ID</span></th>
                    <th><span translate="creamApp.refSecteurActivite.code">Code</span></th>
                    <th><span translate="creamApp.refSecteurActivite.libelle">Libelle</span></th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="refSecteurActivite in refSecteurActivites track by refSecteurActivite.id">
                    <td><a ui-sref="refSecteurActivite.detail({id:refSecteurActivite.id})">{{refSecteurActivite.id}}</a></td>
                    <td>{{refSecteurActivite.code}}</td>
                    <td>{{refSecteurActivite.libelle}}</td>
                    <td>
                        <button type="submit"
                                ui-sref="refSecteurActivite.detail({id:refSecteurActivite.id})"
                                class="btn btn-info btn-sm">
                            <span class="glyphicon glyphicon-eye-open"></span>&nbsp;<span translate="entity.action.view"> View</span>
                        </button>
                        <button type="submit"
                                ui-sref="refSecteurActivite.edit({id:refSecteurActivite.id})"
                                class="btn btn-primary btn-sm">
                            <span class="glyphicon glyphicon-pencil"></span>&nbsp;<span translate="entity.action.edit"> Edit</span>
                        </button>
                        <button type="submit"
                                ui-sref="refSecteurActivite.delete({id:refSecteurActivite.id})"
                                class="btn btn-danger btn-sm">
                            <span class="glyphicon glyphicon-remove-circle"></span>&nbsp;<span translate="entity.action.delete"> Delete</span>
                        </button>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
